{
  "name": "acme-website",
  "version": "1.5.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Oliver",
    "url": "https://oliverpan.vercel.app"
  },
  "scripts": {
    "build": "next build",
    "dev": "dotenv -e .env -- bun run dev:services && next dev --turbopack",
    "dev:next": "next dev --turbopack",
    "dev:services": "docker compose up -d",
    "devsafe": "rm -rf .next && next dev",
    "generate:importmap": "payload generate:importmap",
    "generate:types": "payload generate:types",
    "lint": "biome check",
    "format": "biome format --write",
    "typecheck": "tsc --noEmit",
    "payload": "payload",
    "start": "next start",
    "services:start": "docker compose up -d",
    "services:stop": "docker compose down",
    "services:logs": "docker compose logs -f",
    "services:reset": "docker compose down -v && docker compose up -d",
    "services:status": "docker compose ps",
    "email:start": "docker compose up -d inbucket",
    "email:stop": "docker compose stop inbucket",
    "email:logs": "docker compose logs -f inbucket",
    "email:test": "bun run src/scripts/test-email.ts",
    "s3:start": "docker compose up -d minio",
    "s3:stop": "docker compose stop minio",
    "s3:logs": "docker compose logs -f minio",
    "s3:console": "open http://localhost:9002",
    "db:start": "docker compose up -d postgres",
    "db:stop": "docker compose stop postgres",
    "db:logs": "docker compose logs -f postgres",
    "db:reset": "docker compose down -v postgres && docker compose up -d postgres",
    "db:connect": "docker exec -it acme-postgres psql -U postgres -d acme-website"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@better-auth/passkey": "^1.4.17",
    "@daveyplate/better-auth-ui": "^3.3.15",
    "@payloadcms/db-postgres": "^3.73.0",
    "@payloadcms/email-nodemailer": "^3.73.0",
    "@payloadcms/email-resend": "^3.73.0",
    "@payloadcms/live-preview-react": "^3.73.0",
    "@payloadcms/next": "^3.73.0",
    "@payloadcms/plugin-import-export": "^3.73.0",
    "@payloadcms/plugin-seo": "^3.73.0",
    "@payloadcms/richtext-lexical": "^3.73.0",
    "@payloadcms/storage-s3": "^3.73.0",
    "@payloadcms/ui": "^3.73.0",
    "@react-email/components": "^1.0.6",
    "@react-email/render": "^2.0.4",
    "@tanstack/react-form": "^1.27.7",
    "@vercel/analytics": "^1.6.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "better-auth": "^1.4.17",
    "better-auth-harmony": "^1.2.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "geist": "^1.5.1",
    "graphql": "^16.12.0",
    "html-react-parser": "^5.2.12",
    "lucide-react": "^0.555.0",
    "motion": "^12.29.0",
    "next": "^16.1.4",
    "next-themes": "^0.4.6",
    "nuqs": "^2.8.6",
    "payload": "^3.73.0",
    "payload-auth": "^1.8.4",
    "react": "19.2.1",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.1",
    "react-medium-image-zoom": "^5.4.0",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "resend": "^6.8.0",
    "sharp": "0.34.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.12",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^22.19.7",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "dotenv-cli": "^11.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.9.3",
    "zod": "^4.3.6"
  },
  "engines": {
    "node": ">=22"
  }
}